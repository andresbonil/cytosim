% FJN, 16.08.2016

set simul system
{
    dim = 3
    time_step = 0.0001
    viscosity = 1
    steric = 1, 500
    display = ( size=1024; back_color=black; point_value=0.001; style=3; )
}

set space cell
{
    shape = sphere
    display = ( visible = 0 )
}

new cell
{
    radius = 0.25
}

set fiber microtubule
{
    loop = 1
    rigidity = 10
    segmentation = 0.016
    confine = inside, 0
    steric = 1, 0.020
    display = ( style=microtubule; color=blue; )
}

set fiber actin
{
    loop = 1
    rigidity = 0.05
    segmentation = 0.011
    confine = inside, 0
    steric = 1, 0.010
    display = ( style=actin; color=orange; )
}

set hand binder
{
}

set couple link
{
    hand1 = binder
    hand2 = binder
    stiffness = 10000
}

